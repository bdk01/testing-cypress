{
  "name": "newapp",
  "private": true,
  "version": "0.0.0",
  "type": "commonjs",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "test": "cypress run",
    "open": "cypress open",
    "test-br": "cypress run --browser chrome --headed",
    "test-br-mocha": "yarn test --reporter mochawesome --reporter-options reportDir=\"results\",overwrite=false,html=false,json=true",
    "merge-rp": "mochawesome-merge \"results/*.json\" > mochawesome.json",
    "build-rp": "marge mochawesome.json",
    "open-rp": "yarn merge-rp && yarn build-rp && serve mochawesome-report",
    "lint": "eslint .",
    "preview": "vite preview"
  },
  "dependencies": {
    "@shelex/cypress-allure-plugin": "^2.41.0",
    "mocha": "^11.1.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "serve": "^14.2.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "@types/jest": "^29.5.14",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react": "^4.3.4",
    "cypress": "^14.3.1",
    "eslint": "^9.21.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^15.15.0",
    "marge": "^1.0.1",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^4.4.1",
    "mochawesome-report-generator": "^6.2.0",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.24.1",
    "vite": "^6.2.0"
  }
}
